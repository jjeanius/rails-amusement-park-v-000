<%= form_for @user, url: signin_path(@user) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :nausea %>
    <%= f.text_field :nausea %>

    <%= f.label :happiness %>
      <%= f.text_field :happiness %>

      <%= f.label :height %>
        <%= f.text_field :height %>

        <%= f.label :tickets %>
          <%= f.text_field :tickets %>

  <%= f.label :password %>
  <%= f.text_field :password %>

<%= f.submit "Sign In" %>

<% end %>
